<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Admin: Admin UI</title>
    <!-- Styles -->
     <script src="https://code.jquery.com/jquery-3.4.1.slim.min.js"
        integrity="sha256-pasqAKBDmFT4eHoN2ndd6lN370kFiGUFyTiUHWhU7k8=" crossorigin="anonymous"></script>
    <script src="/javascripts/createTaskList.js"></script>
    <link href="/css/lib/font-awesome.min.css" rel="stylesheet">
    <link href="/css/lib/themify-icons.css" rel="stylesheet">
    <link href="/css/lib/menubar/sidebar.css" rel="stylesheet">
    <link href="/css/lib/bootstrap.min.css" rel="stylesheet">
    <link href="/css/style.css" rel="stylesheet">

</head>

<body>
    <div class="sidebar sidebar-hide-to-small sidebar-shrink sidebar-gestures">
        <div class="nano">
            <div class="nano-content">
                <div class="logo"><a href="index.html"><span>Dashboard</span></a>
                </div>
                <ul>
                    <li><a class="sidebar-sub-toggle"><i class="ti-target"></i> Users <span
                                class="sidebar-collapse-icon ti-angle-down"></span></a>
                        <ul>
                        </ul>
                    </li>
                    
                    <li><a href="sendEmail"><i class="ti-close"></i> send email</a></li>
                    <li><a href="createtasklist"><i class="ti-close"></i> Tasks</a></li>
                    <li><a href="logout"><i class="ti-close"></i> Logout</a></li>
                </ul>
            </div>
        </div>
    </div>
    <!-- /# sidebar -->
    <div class="header">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12">
                    <div class="float-left">
                        <h2>Hello, <span>Welcome Here</span></h2>{{currentUser}}
                    </div>
                  
                </div>
            </div>
        </div>
    </div>
    <div class="content-wrap">
        <div class="main">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-8 p-r-0 title-margin-right">
                        <div class="page-header">
                            <div class="page-title">
                                <h1>Task List</span></h1>
                            </div>
                        </div>
                    </div>
                    <!-- /# column -->
                    <div class="col-lg-4 p-l-0 title-margin-left">
                        <div class="page-header">
                            <div class="page-title">
                                <ol class="breadcrumb">
                                    <li class="breadcrumb-item" ><a href="#">Dashboard</a></li>
                                    <li class="breadcrumb-item active">CreateTaskList</li>
                                </ol>
                            </div>
                        </div>
                    </div>
                    <!-- /# column -->
                </div>
                <!-- /# row -->
                <section id="main-content">
                    <div class="row">
                        <div class="col-lg-8">
                            <div class="card">
                                <div class="card-body">

    <form action="createtasklist" method="POST">
        <input name="tasklistname" placeholder="tasklist name" type="text" required class="taskname"/>
        <button type="submit" id="addtask">Add</button>
        <br />
        <div id="taskFormDiv">

        </div>
    </form>
    <div>

        <table id="tasklist">
            {{#each tasklist}}
            <tr>
                <td align="center"><a href="#" id="taskhref" onclick="postReq('{{this.id}}')">{{this.tasklistname}}</a></td>
                <td align="center" width="10%"><a href="#" id="taskhref" onclick="postReq('{{this.id}}')">Edit</a></td>
                <td align="center" width="10%"><a href="#" id="taskhref" onclick="postReq('{{this.id}}')">Delete</a></td>
                
            </tr>
            {{/each}}
        </table>
    </div>

                                </div>
                               
                              
                                  
                </section>
            </div>
        </div>
    </div>
  
    <!-- jquery vendor -->
  
    <script src="/js/lib/jquery.nanoscroller.min.js"></script>
    <!-- nano scroller -->
    <script src="/js/lib/menubar/sidebar.js"></script>
    <script src="/js/lib/preloader/pace.min.js"></script>
    <!-- sidebar -->
    <script src="/js/lib/bootstrap.min.js"></script>
    <script src="assets/js/scripts.js"></script>
    <!-- scripit init-->

    
</body>

</html>